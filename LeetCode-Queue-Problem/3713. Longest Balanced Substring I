class Solution {
public:
    int longestBalanced(string s) {
        int jasir=s.length();
        int poojith=0;
        for(int i=0;i<jasir;i++){
            vector<int>freq(26,0);
                for(int j=i;j<jasir;j++){
                    freq[s[j]-'a']++;
                    int maxfreq=0;
                    int minfreq=jasir;
                    for(int k=0;k<26;k++){
                        if(freq[k]>0){
                            minfreq=min(minfreq,freq[k]);
                            maxfreq=max(maxfreq,freq[k]);
                    }
                }
                if(minfreq==maxfreq){
                    poojith=max(poojith,j-i+1);
                }
            }
        }
        return poojith;
    }
};
