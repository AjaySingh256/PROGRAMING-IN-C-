//Prime Number
#include<iostream>
using namespace std;
int main()
{
    int n =2;
    bool isprime =true;

    for(int i=2; i<=n-1; i++){
        if(n%i==0){
            //nonprime
            isprime = false;
            break;
        }
    }
    if(isprime== true){
        cout<<"Prime Number\n";
    }else{
        cout<<"Non prime number\n";
    }
    return 0;
}
//***OUTPUT***
//Prime Number
